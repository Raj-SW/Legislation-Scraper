# MauPass credentials
MAUPASS_USERNAME=
MAUPASS_PASSWORD=

# URLs
# Default matches the MauPass login page for Supreme Court portal
MAUPASS_LOGIN_URL=https://maupass.govmu.org/Account/Login/?returnUrl=https%3A//supremecourt.govmu.org/maupass/login
# Webhook that returns the 2FA code
MAUPASS_WEBHOOK_URL=https://n8n.islandai.co/webhook/6a705dfe-98a3-4aec-a7b5-d4fc06e71718

# Browser behavior
# Set to 1 or true to run headless (no window)
MAUPASS_HEADLESS=0

# After-login scraping
MAUPASS_ACTS_URL=https://supremecourt.govmu.org/act-group-lists
# 0 = all pages
MAUPASS_PAGE_LIMIT=0
# Pause between items (seconds)
MAUPASS_PER_ITEM_PAUSE_SECS=0
# Delay after requesting code before fetching webhook (seconds)
MAUPASS_PRE_FETCH_2FA_DELAY_SECS=2
# Post-2FA observation wait (seconds, headed only)
MAUPASS_POST_2FA_WAIT_SECS=8

# Supabase
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_TABLE=
# Optional: direct Postgres URL for creating table on the fly
# Format: postgres://user:pass@host:port/db
SUPABASE_DB_URL=
